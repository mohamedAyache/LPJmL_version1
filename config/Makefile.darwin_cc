CC	= cc
DEBUGFLAGS  = -g
OPTFLAGS= -O3
WFLAG	= -Wall -m64
LPJFLAGS  = -DUSE_RAND48 -DSAFE -DUSE_TGAMMA
O	= o
A	= a
E	=
AR	= ar
ARFLAGS	= r 
RM	= rm 
RMFLAGS	= -f
LINKMAIN= cc -m64
LINK	= cc -m64
MKDIR	= mkdir -p
LIBS	= -lm
SLASH	= /
X11LIB	= -L/usr/X11R6/lib -lX11
VIEWPRG	= lpjserver
